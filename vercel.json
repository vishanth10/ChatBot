  {
    "version": 2,
    "builds": [
      { "src": "chatgpt-backend/index.js", "use": "@vercel/node" },
      { "src": "chatgpt-frontend/package.json", "use": "@vercel/static-build", "config": { "distDir": "chatgpt-frontend/build" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/chatgpt-backend/index.js" },
      { "src": "/(.*)", "dest": "/chatgpt-frontend/$1" }
    ]
  }
